// Виводить повідомлення про версію поточної операційної системи, поточну дату та час.
using System;
using System.Collections.Generic;
using System.Ling;
using System.Ling;
namespace ConsoleApplecation1
{
    class Program
        {
            static void Main(string[] args)
            {
                OperatingSystem os = System.Environment.OSVersion;
                Console.WriteLine("Platform:{0}", os.Platform);
                System.Console.WriteLine("The current date and time is" + System.DateTime.Now);
                System.Console.ReadLine();
            }
        }
}

//Розрахунок функції, значення задаваеться в діалозі з викоританням методу Console.ReadLine()
using System;
using System.Collections.Generic;
using System.Ling;
using System.Text;
namespace lab1
{
    class Program
    {
        static void Main(string[] args)
        {
            double e = 2.718281;
            double pi = 3.14159;
            Console.WriteLine("Input x =\r");
            double x = Convert.ToDouble(ConsoleReadLine());
            double y = (1.3 * Math.Pow(e, -x/2)) + Math.Abs(Math.Cos((2 * pi * x) - 1.4)) - (6/11);
            Math.Round(y, 10);
            Console.WriteLine(" x = {0} /t y = {}} ", x, y);
            Console.ReadKey();
        }  
    }
}
